{
  parser: "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "padded-blocks": 0,
    "no-unused-vars": 0,
    "no-trailing-spaces": 0,
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "no-plusplus": 0,
    "no-multi-spaces": 0,
    "arrow-parens": 1,
  },
  "extends": ["airbnb-base"]
}
