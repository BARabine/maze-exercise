{
  parser: "babel-eslint",
  "env": {
    "jasmine": true,
    "jest": true,
  },
  "plugins": ["react"],
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "curly": 0,
    "function-paren-newline": 0,
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "no-confusing-arrow": 0,
    "no-multi-spaces": 0,
    "no-plusplus": 0,
    "no-trailing-spaces": 0,
    "no-unused-vars": 0,
    "padded-blocks": 0,
  },
  "extends": ["airbnb-base"]
}
